generator client {
  provider = "prisma-client-js"
  seed = "ts-node prisma/seed.ts"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


// --- user.prisma ---

model User {
    id         String   @id @default(cuid())
    email      String   @unique
    username   String   @unique
    password   String
    firstName  String?
    lastName   String?
    createdAt DateTime @default(now())
    updatedAt  DateTime @updatedAt 
}

